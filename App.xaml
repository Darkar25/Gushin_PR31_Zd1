<Application x:Class="Salon.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Salon" xmlns:local1="clr-namespace:Salon.Converters"
             StartupUri="ServicesWindow.xaml">
    <Application.Resources>
        <local1:DiscountVisibility x:Key="discountvisibility" />
        <local1:DiscountDecorations x:Key="discountdecorations" />
        <local1:DiscountColor x:Key="discountcolor" />
        <local1:BoolVisibility x:Key="visibility" />
        <local1:TimeSpanSeconds x:Key="timespanseconds" />
        <Style TargetType="Window">
            <Setter Property="FontFamily" Value="Tahoma" />
        </Style>
    </Application.Resources>
</Application>
